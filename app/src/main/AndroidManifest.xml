<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ifba.tcc_app">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icone_launcher"
        android:label="Meu app"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tcc_app">
        <activity android:name=".controller.CadastroActivity"/>
        <activity android:name=".controller.LoginActivity"/>
        <activity android:name=".controller.ConfiguracaoPart1Activity"/>
        <activity android:name=".controller.ConfiguracaoPart2Activity"/>
        <activity android:name=".controller.TelaPrincipalActivity"/>
        <activity android:name=".controller.TelaPerfilActivity"/>
        <activity android:name=".controller.EditarPerfilActivity"/>
        <activity android:name=".controller.TelaAlterarSenhaActivity"/>
        <activity android:name=".controller.TelaEsqueceuSenhaActivity"/>
        <activity android:name=".controller.LogoffActivity"/>
        <activity android:name=".controller.TelaAcessoRelatorioActivity"/>
        <activity android:name=".controller.ContatoCVVActivity"/>
        <activity android:name=".controller.TelaRelatorioCalendarioActivity"
            android:exported="true" />
        <activity android:name=".controller.TelaRelatorioGraficoActivity"/>
        <activity android:name=".controller.BemVindoPart2Activity"/>
        <activity android:name=".controller.BemVindoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>