<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ifba.tcc_app">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icone_launcher"
        android:label="Meu app"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tcc_app">
        <activity android:name=".CadastroActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".ConfiguracaoActivity"/>
        <activity android:name=".TelaPrincipalActivity"/>
        <activity android:name=".TelaPerfilActivity"/>
        <activity android:name=".EditarPerfilActivity"/>
        <activity android:name=".TelaAlterarSenhaActivity"/>
        <activity android:name=".TelaEsqueceuSenhaActivity"/>
        <activity android:name=".LogoffActivity"/>
        <activity android:name=".TelaRelatoriosActivity"/>
        <activity android:name=".BemVindoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>